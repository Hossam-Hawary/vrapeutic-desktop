{
  "name": "vrapeutic-desktop",
  "productName": "VRapeutic Desktop",
  "description": "VRapeutic desktop app for managing modules",
  "version": "0.0.27",
  "author": {
    "name": "Hossam Eldeen Hawary",
    "email": "hossameldeen.hawary@gmail.com",
    "url": "https://github.com/Hossam-Hawary"
  },
  "copyright": "All rights reserved © VRapeutic",
  "license": "APACHE2",
  "bugs": {
    "url": "https://github.com/Hossam-Hawary/vrapeutic-desktop/issues"
  },
  "keywords": [
    "vr",
    "VRapeutic"
  ],
  "assetsPath": "assets",
  "homepage": "https://github.com/Hossam-Hawary/vrapeutic-desktop",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "electron-builder",
    "electron": "cross-env NODE_ENV=production ng build --prod --base-href ./ && tsc --p electron  && electron .",
    "build-electron": "NODE_ENV=production ng build --prod --base-href ./ && tsc --p electron",
    "package-linux": "electron-packager . Vrapeutic --overwrite --asar=true --platform=linux --arch=x64 --icon=1024x1024.png --prune=true --out=release-builds --extraResource=dist/vrapeutic-desktop/assets/modules/ --ignore=release-builds/",
    "package-win": "electron-packager . Vrapeutic --overwrite --asar=false --platform=win32 --arch=x64 --icon=icon.png.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Vrapeutic Desktop App \" "
  },
  "repository": "https://github.com/Hossam-Hawary/vrapeutic-desktop",
  "publish": "github",
  "build": {
    "appId": "com.myvrapeutic.vrapeutic-desktop",
    "productName": "VRapeutic Desktop",
    "copyright": "All rights reserved © VRapeutic",
    "publish": [
      {
        "provider": "github",
        "owner": "Hossam-Hawary",
        "repo": "vrapeutic-desktop",
        "vPrefixedTagName": true
      }
    ],
    "asar": true,
    "directories": {
      "buildResources": "build",
      "output": "release-builds"
    },
    "dmg": {
      "artifactName": "${productName}-mac-${version}.${ext}",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "appImage": {
      "license": "LICENSE"
    },
    "win": {
      "artifactName": "${productName}-windows-${version}.${ext}",
      "target": "nsis",
      "icon": "build/icon.png.png"
    },
    "nsisWeb": {
      "oneClick": false
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "artifactName": "${productName}-linux-${version}.${ext}",
      "target": [
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/http": "^7.2.15",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@ionic/angular": "^4.7.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "adbkit": "^2.11.1",
    "body-parser": "^1.19.0",
    "capture-console": "^1.0.1",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "electron-debug": "^3.0.0",
    "electron-log": "^3.0.1",
    "electron-settings": "^2.2.4",
    "electron-updater": "^4.2.0",
    "fix-path": "^2.1.0",
    "enum": "^2.5.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "internal-ip": "^4.3.0",
    "ng2-charts": "^2.2.3",
    "ngx-electron": "^2.1.1",
    "pusher-js": "^4.4.0",
    "rxjs": "~6.3.3",
    "shortid": "^2.2.14",
    "socket.io": "^2.2.0",
    "tslib": "^1.9.0",
    "xlsx": "^0.15.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@ionic/angular-toolkit": "^2.0.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.2",
    "codelyzer": "~4.5.0",
    "electron": "^7.1.2",
    "electron-builder": "^22.1.0",
    "electron-builder-lib": "^20.15.3",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^14.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "cross-env": "^6.0.0"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=5.x"
  }
}
